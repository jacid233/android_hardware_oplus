# Allow system_app to read and write to sysfs_dc
allow system_app sysfs_dc:file rw_file_perms;

# Allow system_app to read and write to sysfs_hbm
allow system_app sysfs_hbm:file rw_file_perms;

allow system_app vendor_proc_display:dir search;
allow system_app vendor_proc_display:file { getattr open read write };

allow system_app vendor_sysfs_graphics:dir search;
allow system_app vendor_sysfs_graphics:file { getattr open read write };

allow system_app vendor_sysfs_usb_supply:dir search;
